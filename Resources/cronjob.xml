<?xml version="1.0" encoding="utf-8"?>
<cronjobs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/shopware/shopware/5.3/engine/Shopware/Components/Plugin/schema/cronjob.xsd">

    <cronjob>
        <name>Berechne Recommender Artikel</name>
        <action>CalculateRecommenderArticlesCron</action>
        <active>true</active>
        <interval>604800</interval>
        <disableOnError>false</disableOnError>
    </cronjob>

    <cronjob>
        <name>Berechne Recommender Artikel Plentymarkets</name>
        <action>CalculateRecommenderArticlesPlentymarketsCron</action>
        <active>true</active>
        <interval>604800</interval>
        <disableOnError>false</disableOnError>
    </cronjob>

    <cronjob>
        <name>PlentyMarkets Import</name>
        <action>GetPlentyMarketsOrdersCron</action>
        <active>true</active>
        <interval>86400</interval>
        <disableOnError>false</disableOnError>
    </cronjob>

</cronjobs>